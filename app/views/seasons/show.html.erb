<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li><%= link_to 'New season', new_season_path %></li>
      <li><%= link_to 'Delete', @season, :confirm => 'Are you sure?', :method => :delete %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">
  	<h2 class="title"><%="Season: "+h(@season.name) %></h2>
    <div class="inner">      
       <table class="table">                      
      	<tr>
      	  <th class="first">Name</th>
      	  <th class="last"></th>
      	</tr>              	
      	<% @season.rounds.each do |round| %>
      	  <tr>              	              	  
      	    <td><%=link_to h("Round: "+round.name), season_round_path(@season, round) %></td>
      	    <td class="last"><%=link_to "edit", edit_season_round_path(@season, round) %></td>              	                  	  </tr>
    	<% end -%>              	
  	  </table>	          	    
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>